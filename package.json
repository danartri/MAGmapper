    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
